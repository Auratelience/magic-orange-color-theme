{
  "$schema": "vscode://schemas/color-theme",
  "type": "dark",
  "name": "MagicOrange",

  // UI colors
  "colors": {
    "foreground": "#f8f8f0",
    "activityBar.foreground": "#f8f8f2",
    "activityBar.background": "#212121",
    "activityBar.activeBorder": "#EE5A24",
    "activityBarBadge.background": "#ff5100",
    "button.background": "#ff5100",
    "dropdown.background": "#31332e",
    "focusBorder": "#EE5A24",
    "input.background": "#212121",
    "list.activeSelectionBackground": "#31332e",
    "list.dropBackground": "#31332e",
    "list.focusBackground": "#1b1d1a",
    "list.hoverBackground": "#1b1d1a",
    "list.inactiveSelectionBackground": "#31332e",
    "menu.background": "#212121",
    "menu.selectionBackground": "#833d1d",
    "scrollbarSlider.background": "#31332e",
    "selection.background": "#949690",
    "sideBar.background": "#212121",
    "statusBar.background": "#EE5A24",
    "statusBar.noFolderBackground": "#EE5A24",
    "textLink.activeForeground": "#833d1d",
    "textLink.foreground": "#EE5A24",
    "titleBar.activeBackground": "#212121",
    "titleBar.inactiveBackground": "#212121",
    "editor.background": "#212121",
    "editor.foreground": "#d1d8e0",
    "editor.lineHighlightBackground": "#31332e",
    "editor.selectionBackground": "#31332e",
    "editorCursor.foreground": "#f8f8f0",
    "editorGroupHeader.tabsBackground": "#212121",
    "editorWhitespace.foreground": "#31332e",
    "window.activeBorder": "#EE5A24",
    "window.inactiveBorder": "#212121",
    "badge.background": "#EE5A24",
    "badge.foreground": "#f8f8f2",
    "checkbox.border": "#ff5100",
    "tab.inactiveBackground": "#212121"
  },
  // Code Colors
  "tokenColors": [
    // general colors
    {
      "name": "String",
      "scope": "string",
      "settings": {
        "foreground": "#1EB322"
      }
    },
    {
      "name": "Number",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#66D9EF"
      }
    },
    {
      "name": "Variable",
      "scope": "variable",
      "settings": {
        "foreground": "#FCB065"
      }
    },
    {
      "name": "Function name",
      "scope": "entity.name.function",
      "settings": {
        "foreground": "#1dd1a1"
      }
    },
    {
      "name": "Function argument",
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#AE81FF"
      }
    },

    // CSS Colors

    // HTML Colors
    {
      "name": "Tag name",
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#79E99B"
      }
    },
    {
      "name": "Tag attribute",
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#35E99B"
      }
    },

    // Constant Colors
    {
      "name": "Built-in constant",
      "scope": "constant.language",
      "settings": {
        "foreground": "#E6583E"
      }
    },
    {
      "name": "User-defined constant",
      "scope": ["constant.character", "constant.other"],
      "settings": {
        "foreground": "#E6583E"
      }
    },

    {
      "name": "Keyword",
      "scope": "keyword",
      "settings": {
        "foreground": "#E6583E"
      }
    },

    {
      "name": "Template string expression",
      "scope": "meta.template.expression",
      "settings": {
        "foreground": "#f8f2f2"
      }
    },
    {
      "name": "Comment",
      "scope": "comment",
      "settings": {
        "foreground": "#75715E"
      }
    },

    {
      "name": "Storage",
      "scope": "storage",
      "settings": {
        "foreground": "#FD971F"
      }
    },
    {
      "name": "Storage type",
      "scope": "storage.type",
      "settings": {
        "foreground": "#EE5A24"
      }
    },
    {
      "name": "CSS Class and Support",
      "scope": [
        "source.css support.type.property-name",
        "source.sass support.type.property-name",
        "source.scss support.type.property-name",
        "source.less support.type.property-name",
        "source.stylus support.type.property-name",
        "source.postcss support.type.property-name"
      ],
      "settings": {
        "foreground": "#FD971F"
      }
    },
    {
      "name": "Class name",
      "scope": "entity.name.class",
      "settings": {
        "foreground": "#A6E22E"
      }
    },
    {
      "name": "Inherited class",
      "scope": "entity.other.inherited-class",
      "settings": {
        "foreground": "#A6E22E"
      }
    },

    {
      "name": "Library function",
      "scope": "support.function",
      "settings": {
        "foreground": "#1dd1a1"
      }
    },
    {
      "name": "Library constant",
      "scope": "support.constant",
      "settings": {
        "foreground": "#66D9EF"
      }
    },
    {
      "name": "Library class/type",
      "scope": ["support.type", "support.class"],
      "settings": {
        "foreground": "#1dd1a1"
      }
    },
    {
      "name": "Library variable",
      "scope": "support.other.variable",
      "settings": {}
    },
    {
      "name": "Invalid",
      "scope": "invalid",
      "settings": {
        "foreground": "#F8F8F0"
      }
    },
    {
      "name": "Invalid deprecated",
      "scope": "invalid.deprecated",
      "settings": {
        "foreground": "#F8F8F0"
      }
    },
    {
      "scope": ["punctuation.separator", "punctuation.accessor"],
      "settings": {
        "foreground": "#EE5A24"
      }
    }
  ]
}
